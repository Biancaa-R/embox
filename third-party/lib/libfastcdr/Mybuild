package third_party.lib

@BuildArtifactPath(cppflags="-I$(EXTERNAL_BUILD_DIR)/third_party/lib/libfastcdr/install/include")
@Build(script="$(EXTERNAL_MAKE)")
@BuildDepends(third_party.gcc.libstdcxx)
@BuildDepends(third_party.gcc.libsupcxx)
module libfastcdr {
	@AddPrefix("^BUILD/extbld/^MOD_PATH/install/lib")
	source "libfastcdr.a"

	@NoRuntime depends third_party.gcc.libstdcxx
	@NoRuntime depends third_party.gcc.libsupcxx
}
